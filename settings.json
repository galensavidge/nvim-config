{
    "extensions.experimental.affinity": {
        "asvetliakov.vscode-neovim": 1
    },
    "terminal.integrated.commandsToSkipShell": [
        "language-julia.interrupt"
    ],
    "julia.symbolCacheDownload": true,
    "julia.enableTelemetry": false,
    "workbench.colorTheme": "Catppuccin Mocha",
    "editor.fontSize": 12,
    "debug.console.fontSize": 12,
    "terminal.integrated.fontSize": 12,
    "chat.editor.fontSize": 12,
    "markdown.preview.fontSize": 12,
    "keyboard.dispatch": "keyCode",
    "window.customMenuBarAltFocus": false,
    "window.enableMenuBarMnemonics": false,
    "editor.minimap.renderCharacters": false,
    "editor.minimap.autohide": true,
    "editor.autoClosingBrackets": "never",
    "editor.autoSurround": "never",
    "editor.wordWrap": "on",
    "editor.wrappingIndent": "deepIndent",
    "editor.scrollBeyondLastColumn": 0,
    "editor.smoothScrolling": true,
    "editor.minimap.enabled": false,
    "editor.acceptSuggestionOnCommitCharacter": false,
    "editor.inlineSuggest.enabled": false,
    "editor.suggest.preview": true,
    "vscode-neovim.NVIM_APPNAME": "nvim-vscode",
    "editor.suggestOnTriggerCharacters": false,
    "terminal.integrated.suggest.suggestOnTriggerCharacters": false,
    "editor.codeActionWidget.includeNearbyQuickFixes": false,
    "editor.guides.highlightActiveBracketPair": false,
    "editor.occurrencesHighlightDelay": 500,
    "editor.occurrencesHighlight": "multiFile",
    "editor.selectionHighlight": false,
    "editor.renderWhitespace": "none",
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.bracketPairColorization.enabled": false,
    "gitlens.plusFeatures.enabled": false,
    "gitlens.currentLine.enabled": false,
    "editor.lightbulb.enabled": "off",
    "editor.matchBrackets": "near",
    "editor.quickSuggestions": {
        "other": "off"
    },
    "workbench.settings.applyToAllProfiles": [
        "editor.quickSuggestions",
        "editor.suggestOnTriggerCharacters"
    ],
    "gitlens.codeLens.enabled": false,
    "editor.lineNumbers": "relative",
    "editor.inlineSuggest.suppressSuggestions": true,
    "editor.autoClosingComments": "never",
    "editor.autoClosingQuotes": "never",
    "editor.suggest.localityBonus": true,
    "editor.suggest.matchOnWordStartOnly": false,
    "editor.rulers": [
        80
    ],
    "[python]": {
        "editor.rulers": [
            80
        ]
    },
    "[julia]": {
        "editor.rulers": [
            93
        ]
    },
    "terminal.external.windowsExec": "pwsh.exe",
    "terminal.integrated.allowChords": false,
    "terminal.external.linuxExec": "bash",
    "terminal.integrated.fontLigatures.enabled": true,
    "terminal.integrated.smoothScrolling": true,
    "editor.quickSuggestionsDelay": 1000000000
}